
# php -- BEGIN cPanel-generated handler, do not edit
# Set the “ea-php56” package as the default “PHP” programming language.
<IfModule mime_module>
  AddType application/x-httpd-ea-php56___lsphp .php .php5 .phtml
</IfModule>
# php -- END cPanel-generated handler, do not edit


# if using www.
RewriteCond %{HTTP_HOST} ^www.kurtlourens.com$ [NC]
RewriteRule ^(.*)$ https://kurtlourens.com%{REQUEST_URI} [R=301,NC,L,QSA]

# if trying to get to index.html
RewriteCond %{REQUEST_URI} index.html
RewriteRule .? https://kurtlourens.com [R=301,L] 

# just https in general
RewriteEngine On 
RewriteCond %{SERVER_PORT} 80 
RewriteRule ^(.*)$ https://kurtlourens.com/$1 [R,L]