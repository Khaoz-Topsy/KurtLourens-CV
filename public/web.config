<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <httpProtocol>
            <customHeaders>
                <add name="Content-Security-Policy" value="default-src 'none'; script-src 'self' 'report-sample' 'unsafe-inline' platform.twitter.com www.google-analytics.com; style-src 'self' 'report-sample' 'unsafe-inline' nmscd.github.io fonts.googleapis.com; img-src 'self' data: cdn.assistantapps.com blog.kurtlourens.com khaoznet.visualstudio.com vsrm.dev.azure.com khaoznet.vsrm.visualstudio.com; font-src 'self' data: fonts.gstatic.com; connect-src api.assistantapps.com playground.kurtlourens.com; manifest-src https://kurtlourens.com/manifest.json https://playground.kurtlourens.com/manifest.json; base-uri 'self'; form-action 'none'; report-uri 'none'; frame-ancestors 'none'; upgrade-insecure-requests;" />
                <add name="X-Content-Security-Policy" value="default-src 'none'; script-src 'self' 'report-sample' 'unsafe-inline' platform.twitter.com www.google-analytics.com; style-src 'self' 'report-sample' 'unsafe-inline' nmscd.github.io fonts.googleapis.com; img-src 'self' data: cdn.assistantapps.com blog.kurtlourens.com khaoznet.visualstudio.com vsrm.dev.azure.com khaoznet.vsrm.visualstudio.com; font-src 'self' data: fonts.gstatic.com; connect-src api.assistantapps.com playground.kurtlourens.com; manifest-src https://kurtlourens.com/manifest.json https://playground.kurtlourens.com/manifest.json; base-uri 'self'; form-action 'none'; report-uri 'none'; frame-ancestors 'none'; upgrade-insecure-requests;" />
                <add name="X-WebKit-CSP" value="default-src 'none'; script-src 'self' 'report-sample' 'unsafe-inline' platform.twitter.com www.google-analytics.com; style-src 'self' 'report-sample' 'unsafe-inline' nmscd.github.io fonts.googleapis.com; img-src 'self' data: cdn.assistantapps.com blog.kurtlourens.com khaoznet.visualstudio.com vsrm.dev.azure.com khaoznet.vsrm.visualstudio.com; font-src 'self' data: fonts.gstatic.com; connect-src api.assistantapps.com playground.kurtlourens.com; manifest-src https://kurtlourens.com/manifest.json https://playground.kurtlourens.com/manifest.json; base-uri 'self'; form-action 'none'; report-uri 'none'; frame-ancestors 'none'; upgrade-insecure-requests;" />
                <add name="Set-Cookie" value="cross-site-cookie=name; SameSite=None; Secure" />
            </customHeaders>
        </httpProtocol>
        <rewrite>
            <rules>
                <rule name="RedirectToDiscord" stopProcessing="true">
                    <match url="^(.*)Discord(.*)" ignoreCase="true" />
                    <action type="Redirect" url="https://discord.gg/q3aFBQM?ref=kurtlourens.com" appendQueryString="false" redirectType="Permanent" />
                </rule>
                <rule name="RedirectToGithub" stopProcessing="true">
                    <match url="^(.*)Github(.*)" ignoreCase="true" />
                    <action type="Redirect" url="https://github.com/AssistantApps?ref=kurtlourens.com" appendQueryString="false" redirectType="Permanent" />
                </rule>
                <rule name="RedirectToKurt" stopProcessing="true">
                    <match url="^(.*)Kurt(.*)" ignoreCase="true" />
                    <action type="Redirect" url="https://kurtlourens.com?ref=kurtlourens.com" appendQueryString="false" redirectType="Permanent" />
                </rule>
            </rules>
        </rewrite>
    </system.webServer>
</configuration>